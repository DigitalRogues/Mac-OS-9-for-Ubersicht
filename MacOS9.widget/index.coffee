zoom			= "120%" #100% is Apple's original size. 120% looks better on larger screens, but tweak this as your mileage may vary
ostext			= "Mac OS 9"
#------------------#
status			= "Welcome to Mac OS" #"Welcome to Mac OS" was the first default screen. "Starting Upâ€¦" was shown during boot-up.
barwidth		= "3%" #If you want this "stuck" in a static position, put a percentage here
showprogress	= true #true = show the progress bar, false = hide it
showclock		= true #show the clock instead of the standard startup stuff (setting true will override the status, barwidth and showprogress vars)
t24hourtime		= false #true = clock uses 24-hour time (default: false = 12-hour time)



style: """

	/*POSITION*/
	width:100%
	height:100%

	font-smoothing: antialiased
	text-rendering: optimizeLegibility

	#desktop-bg
		position:absolute
		top:0
		left:0
		width:100%
		height:100%
		background-color:#888
		background:#888 url('MacOS9.widget/images/Mac OS Default.png') repeat 0 0
	#MacOS9
		position:absolute
		width:422px
		height:323px
		border-top:1px solid #FFF
		border-right:1px solid #a0a0a0
		border-bottom:1px solid #a0a0a0
		border-left:1px solid #FFF
		outline:1px solid #000
		background:#d8d8d8
		top:33%
		left:50%
		transform:translate(-50%,-50%)
		text-align:center
		zoom:#{zoom}
		//hbox-shadow:5px 5px 10px rgba(#000,.25)
	#MacOS9 .finder
		position:relative
		height:100%
	.finder .logo
		position:relative
		top:24px
		left:50%
		transform:translateX(-50%)
		width:347px
		height:220px
		background:#fff
		border-top:1px solid #a0a0a0
		border-right:1px solid #fff
		border-bottom:1px solid #fff
		border-left:1px solid #a0a0a0
	.finder .logo .logo-inner
		display:block
		position:absolute
		top:0
		right:0
		bottom:0
		left:0
		border:1px solid #000
	.finder H1
		position:absolute
		top:63%
		left:0
		width:100%
		margin:auto
		font:normal 4.3em/1 'apple garamond'
		text-align:center
		letter-spacing:-1.5px
		color:#000
		white-space:nowrap
	.finder svg
		position:absolute
		width:109px
		height:auto
		top:17px
		left:50%
		transform:translateX(-50%)
		display:block
	.finder .loading H3
		position:absolute
		top:80%
		width:100%
		text-align:center
		font:normal .75em/1 'charcoal'
		color:#000
	.finder .loading .progress
		position:absolute
		top:90%
		left:50%
		transform:translateX(-50%)
		width:160px
		height:12px
		background:#b3b3b3
		outline:1px solid #000
		outline-offset:-1px
		border-top:1px solid #a0a0a0
		border-right:1px solid #fff
		border-bottom:1px solid #fff
		border-left:1px solid #a0a0a0
	.finder .loading .progress:before
		content:''
		display:block
		position:absolute
		top:1px
		left:1px
		width:156px
		height:8px
		border-top:1px solid #7d7d7d
		border-right:1px solid #d8d8d8
		border-bottom:1px solid #d8d8d8
		border-left:1px solid #7d7d7d
	.finder .loading .progress .bar
		position:absolute
		top:1px
		left:1px
		width:auto
		height:11px
		background:linear-gradient(to bottom,#2c2d8f 0%,#5b5bc5 11%,#8f8fff 22%,#c5c5ff 33%,#ecebec 44%,#c5c5ff 56%,#8f8fff 67%,#5b5bc5 78%,#2c2d8f 89%,#00004a 100%);
		transition:width 1s
	.finder .loading .progress .bar:before
		content:''
		position:absolute
		top:1px
		left:0
		width:1px
		height:7px
		border-left:1px solid #5b5bc5
		background:linear-gradient(to bottom,#5b5bc5 0,#8f8fff 11%,#c5c5ff 22%,#ecebec 33%,#ecebec 56%,#c5c5ff 67%,#8f8fff 78%,#5b5bc5 89%,#2c2d8f 100%);
	.finder .loading .progress .bar:after
		content:''
		position:absolute
		top:0
		right:0
		width:1px
		height:10px
		border-left:1px solid #2c2d8f
		border-right:1px solid #000
		background:#00004a
		box-shadow:1px 0 1px #4b4b4b
	#eye-click
		width:100%
		height:20%
		position:absolute
		bottom:0
		z-index:2

