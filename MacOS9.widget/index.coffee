afterRender: (domEl) ->
	$(domEl).on 'click', '#eye-click', =>
		# @run "say -v Alex 'I am completely operational and all my circuits are functioning perfectly.'"
		@run $(domEl).find('#audio1').get(0).play()

addZero: (i) ->
	if(i < 10)
		i = "0" + i
	return i

update: (output,domEl) ->
	$domEl = $(domEl)

	if showprogress == true
		$('.progress').css("display","block")
	else
		$('.progress').css("display","none")

	if showclock == true
		$('.progress').css("display","block")
		now = new Date()
		d = now.getDay()
		h = @addZero(now.getHours())
		m = @addZero(now.getMinutes())
		s = now.getSeconds()
		sp = Math.round((s / 60) * 100)
		meridian = h >= 12 ? 'PM' : 'AM'
		if t24hourtime==false
			if h >= 12
				h = h - 12
			if h == 0
				h = 12
		if meridian == true
			meridian= 'PM'
		else
			meridian = 'AM'
		weekday = new Array(7)
		weekday[0] = "Sun"
		weekday[1] = "Mon"
		weekday[2] = "Tue"
		weekday[3] = "Wed"
		weekday[4] = "Thu"
		weekday[5] = "Fri"
		weekday[6] = "Sat"
		d = weekday[d]

		$('#status').text(d+' '+h+':'+m+' '+meridian)

		if s == 0
			$('#macprogress').css("display","none")
		else
			$('#macprogress').css("display","block")
			$('#macprogress').css("width","#{sp}%")
	else
		$('#status').text(status)
		$('#macprogress').css("width","#{barwidth}%")

		# $('#macprogress').text("#{s}%")

